{
  "family": "one-dao-be-td",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::973530737784:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "one-dao-cntr",
      "image": "placeholder-image-updated-by-ci",
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "database-one-dao-instance-1.cxmw0mi0czbu.ap-south-1.rds.amazonaws.com"
        },
        {
          "name": "DB_USER",
          "value": "onedbuser"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:973530737784:secret:your-db-password-name"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/one-daoService-be",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
