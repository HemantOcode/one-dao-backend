{
  "family": "one-dao-be-td",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::973530737784:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "one-dao-cntr",
      "image": "placeholder-image-updated-by-ci",
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "your-rds-endpoint.ap-south-1.rds.amazonaws.com"
        },
        {
          "name": "DB_USER",
          "value": "onedbuser"
        },
        {
          "name": "DB_PASSWORD",
          "value": "Password123!"
        }
      ]
    }
  ]
}